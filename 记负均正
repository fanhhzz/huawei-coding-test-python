while True:
    try:
        a=int(input())
        b=list(map(int,input().split()))
        c=0
        d=0
        dd=0
        for i in b:
            if i<0:
                c=c+1
            elif i>0:
                d=d+i 
                dd+=1
        w=round(d/dd*10)
        print(str(c)+" "+str(w/10))
    except:break
