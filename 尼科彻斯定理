#n+n+2m-2=2*m^2
#n=m^2-m+1
while True:
    try:
        m=input()
        m=int(m)
        n=range(m*m-m+1,m*m+m+1,2)
        #print(n)
        i=1
        #b=[]
        #print(n[i])
        c=str(n[0])+'+'
        while i<(m-1):
            temp=str(n[i])+'+'
            c=c+temp
            
            i=i+1
        #b.append(str(n[m-1]))
        b=(m-1)
        c=c+str(n[b])
        print(c)
    except:break


