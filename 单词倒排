while True:
    try:
        a1=input()
        a2=''
        for i in range(len(a1)):
            if (97<=ord(a1[i])<=122) or (65<=ord(a1[i])<=90):
                a2=a2+a1[i]
            else:
                a2=a2+' '
        #print(a2)
        a=list(a2.split())
        b=a[::-1]
        print(" ".join(b))
    except:break
###########

a=list(input().split())
b=a[::-1]
print(" ".join(b))
